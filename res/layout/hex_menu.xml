<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	android:id="@+id/widget131"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:padding="40px"
	android:gravity="center"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
	
	<LinearLayout
		android:id="@+id/widget76"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:gravity="center"
		>
		
		<!-- LeftMost edges -->
		<LinearLayout
			android:id="@+id/widget76"
			android:gravity="center_vertical"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical"
			>
			
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_left"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_left"
			/>
			
		</LinearLayout>
		
		<!-- First collumn -->
		<LinearLayout
			android:id="@+id/widget76"
			android:gravity="center_vertical"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical"
			>
			
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center|right"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_for_slash"
			/>
			<Button
			android:id="@+id/widget75"
			android:layout_gravity="center"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:background="@drawable/hex_middle"
			android:text="test"
			android:textColor="#00ff00"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_middle"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center|right"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_back_slash"
			/>
		</LinearLayout>
		
		<!-- Second collumn -->
		<LinearLayout
			android:id="@+id/widget76"
			android:gravity="center_vertical"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical"
			>
			<ImageView
			android:id="@+id/widget75"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_middle"
			>
			</ImageView>
			<ImageView
			android:id="@+id/widget75"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_middle"
			>
			</ImageView>
			<ImageView
			android:id="@+id/widget75"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_middle"
			>
			</ImageView>
		</LinearLayout>
		
		<!-- Third collumn -->
		<LinearLayout
			android:id="@+id/widget76"
			android:gravity="center_vertical"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical"
			>
			
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center|left"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_back_slash"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_middle"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_middle"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center|left"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_for_slash"
			/>
		</LinearLayout>
		
		<!-- RightMost edges -->
		<LinearLayout
			android:id="@+id/widget76"
			android:gravity="center_vertical"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical"
			>
			
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_right"
			/>
			<ImageView
			android:id="@+id/widget75"
			android:layout_gravity="center"
			android:layout_width="100dip"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/hex_right"
			/>
			
		</LinearLayout>
		
	</LinearLayout>

</LinearLayout>
