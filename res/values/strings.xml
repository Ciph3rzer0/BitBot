    <!-- Sequence, Selection, Iteration-->
    <string name="for_statement">FOR I 1 TO N \n\t&lt;executed statement&gt;\nNEXT I\n</string>
    <string name="if_statement">IF &lt;some condition&gt; THEN GOTO &lt;some line&gt;\n</string>
    <string name="var_declaration">DIM VAR &lt;variable declaration&gt; as &lt;TYPE&gt;\n</string>
    <string name="do_while_statement">DO \n\t&lt;executed statements&gt;\nLOOP UNTIL/WHILE\n</string>
    <string name="print_statement">PRINT &lt;String&gt; ; &lt;variable&gt;\n</string>
   
    <!-- Quick Tools -->
    <string name="parenthesis">( )\n</string>
    <string name="quotations">"\" \""\n</string>
    <string name="braces">{ }\n</string>
    <string name="brackets">[ ]\n</string>
    
    <!-- Bot Functions -->
    <string name="bot_move">call bot_move(int x, int y)\n{\n\t//move code here\n}\n</string>
    <string name="turret_rotate">call turret_rotate(int degree)\n{\n\t//rotation code here\n}\n</string>
    
</resources>
